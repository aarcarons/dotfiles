[include]
        path = .gitconfig.local
[core]
       editor = emacsclient
       quotepath = false
       autocrlf = false
[color]
        status = auto
        diff = auto
	branch = auto
	interactive = auto
	grep = auto
[alias]
        a  = add
	st = status
        sm = submodule
        ft = fetch
	co = checkout
	br = branch
        ba = branch -a
        pr = pull --rebase
        # commit
	c  = commit
        cm = commit -m
        cam = commit -a -m
        came = commit -a --amend
        # log
        so = remote show origin
        sh = show
	log-graph = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
	log-all = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
        # diff
	di = diff
        dm = diff master
        dc = diff --cached
        d1 = diff HEAD~